SELECT DISTINCT ?film ?filmLabel ?genreLabel ?countryLabel ?duration ?capital_cost
WHERE {
  ?film wdt:P31 wd:Q202866 ;  # Экземпляры мультфильмов (Q202866)
        wdt:P136 ?genre ;     # Жанр мультфильма
        wdt:P495 ?country ;   # Страна происхождения
        wdt:P2047 ?duration;  # Продолжительность (в минутах)
        wdt:P2130 ?capital_cost. # Затраты на мультфильм (budget)

  SERVICE wikibase:label { bd:serviceParam wikibase:language "ru" }
}
